{
  "regions": [
    "gru1"
  ],
  "functions": {
    "app/api/companies/search/route.ts": {
      "maxDuration": 5
    },
    "app/api/companies/enrich/route.ts": {
      "maxDuration": 60
    },
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_SKIP_AUTOINSTALL": "false"
    }
  },
  "crons": [
    {
      "path": "/api/cron/reingest",
      "schedule": "0 */6 * * *"
    }
  ]
}